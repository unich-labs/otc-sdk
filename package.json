{
    "name": "otc-sdk",
    "version": "0.0.1",
    "description": "OTC SDK",
    "license": "UNLICENSED",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/sdk.esm.js",
    "umd:main": "dist/sdk.umd.production.min.js",
    "unpkg": "dist/sdk.umd.production.min.js",
    "jsdelivr": "dist/sdk.umd.production.min.js",
    "jsnext:main": "dist/sdk.esm.js",
    "react-native": "dist/sdk.esm.js",
    "source": "src/index.ts",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/sdk.esm.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./*": "./*"
    },
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "clean": "rimraf dist",
        "build": "yarn clean && rollup --config --bundleConfigAsCjs",
        "build:doc": "rimraf docs && typedoc --plugin typedoc-plugin-markdown --out docs src/index.ts --readme none",
        "test": "ts-mocha -r ts-node/register test/*.spec.ts --timeout 200000"
    },
    "publishConfig": {
        "@scope:registry": "https://gitlab.unich.com/api/v4/projects/23/packages/npm/"
    },
    "dependencies": {
        "@coral-xyz/anchor": "^0.29.0",
        "@solana/spl-token": "^0.4.3",
        "@solana/web3.js": "^1.91.7",
        "@unich-lab/otc-sdk": "file:../otc-sdk",
        "ethers": "^6.12.0",
        "lodash": "^4.17.21"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.8",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-replace": "^5.0.7",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/chai": "^4.3.0",
        "@types/chai-as-promised": "^7.1.8",
        "@types/expect": "^24.3.0",
        "@types/lodash": "^4.17.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "^20.14.14",
        "chai": "^4.3.4",
        "chai-as-promised": "^7.1.1",
        "cross-env": "^7.0.3",
        "mocha": "^9.0.3",
        "rimraf": "^5.0.5",
        "rollup": "^4.14.3",
        "rollup-plugin-polyfill-node": "^0.13.0",
        "ts-mocha": "^10.0.0",
        "ts-node": "^10.0.0",
        "typedoc": "^0.26.5",
        "typedoc-plugin-markdown": "^4.2.3",
        "typescript": "^5.4.5"
    },
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": false
    }
}
