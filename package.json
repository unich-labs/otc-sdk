    "types": "./dist/cjs/index.d.ts",
    "scripts": {
        "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
        "build": "yarn clean && tsc -p tsconfig.cjs.json && rollup --config --bundleConfigAsCjs",
