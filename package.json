        "rollup-plugin-polyfill-node": "^0.13.0",
        "ts-mocha": "^10.0.0",
        "ts-node": "^10.0.0",
        "typedoc": "^0.26.5",
        "typedoc-plugin-markdown": "^4.2.3",
        "typescript": "^5.4.5"
    },
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": false
