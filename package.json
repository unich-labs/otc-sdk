{
    "name": "@unich-lab/otc-sdk",
    "version": "0.0.1",
    "description": "OTC SDK",
    "license": "UNLICENSED",
    "type": "module",
    "dependencies": {
        "@coral-xyz/anchor": "^0.29.0",
        "@solana/spl-token": "^0.4.3",
        "@solana/web3.js": "^1.91.7",
        "@unich-lab/otc-sdk": "file:../otc-sdk",
        "ethers": "^6.12.0",
        "lodash": "^4.17.21"
    },
    "module": "./dist/esm/index.js",
    "main": "./dist/cjs/index.js",
    "browser": "./dist/browser/index.js",
    "types": "./dist/cjs/index.d.ts",
    "scripts": {
        "pre-build:node": "rimraf dist && rimraf tsconfig.tsbuildinfo && rimraf docs",
        "build:node": "yarn pre-build:node && tsc && tsc -p tsconfig.cjs.json",
        "build:browser": "rollup --config --bundleConfigAsCjs",
        "build:doc": "typedoc --plugin typedoc-plugin-markdown src/index.ts",
        "build": "yarn build:node && yarn build:browser && yarn build:doc",
        "test": "mocha 'tests/**/*.{ts,js}' --exit"
    },
    "publishConfig": {
        "@scope:registry": "https://gitlab.unich.com/api/v4/projects/23/packages/npm/"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.8",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-replace": "^5.0.7",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/chai": "^4.3.12",
        "@types/chai-as-promised": "^7.1.8",
        "@types/expect": "^24.3.0",
        "@types/lodash": "^4.17.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "^20.12.7",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "cross-env": "^7.0.3",
        "mocha": "^10.0.0",
        "rimraf": "^5.0.5",
        "rollup": "^4.14.3",
        "rollup-plugin-polyfill-node": "^0.13.0",
        "ts-node": "^10.9.2",
        "typedoc": "^0.26.5",
        "typedoc-plugin-markdown": "^4.2.3",
        "typescript": "^5.4.5"
    },
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": false
    }
